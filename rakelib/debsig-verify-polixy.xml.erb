<?xml version="1.0"?>
<!DOCTYPE Policy SYSTEM "https://www.debian.org/debsig/1.0/policy.dtd">
<Policy xmlns="https://www.debian.org/debsig/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://www.debian.org/debsig/1.0/ ">
    <Origin Name="gocd" id="<%= GPG_SIGNING_ID %>" Description="GoCD Signer"/>

    <Selection>
        <Required Type="origin" File="debsig.gpg" id="<%= GPG_SIGNING_ID %>"/>
    </Selection>

    <Verification MinOptional="0">
        <Required Type="origin" File="debsig.gpg" id="<%= GPG_SIGNING_ID %>"/>
    </Verification>
</Policy>

